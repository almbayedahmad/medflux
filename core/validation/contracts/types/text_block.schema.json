{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Text Block",
  "type": "object",
  "properties": {
    "page": { "type": "integer", "minimum": 1 },
    "text": { "type": "string" },
    "bbox": {
      "type": "array",
      "items": { "type": "number" },
      "minItems": 4,
      "maxItems": 4
    }
  },
  "required": ["page", "text"],
  "additionalProperties": true
}
